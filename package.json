{
  "name": "ng2-youtube",
  "version": "0.0.1",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "lite": "lite-server -c bs-config.js",
    "ngc": "ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup.config.js",
    "es5": "tsc --target es5 --allowJs dist/bundle.rollup.js --out dist/bundle.js",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "build_production": "npm run ngc && npm run rollup && npm run es5 && npm run minify"
  },
  "author": "Gmanpark@2016",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.3.0",
    "@angular/compiler": "^2.3.0",
    "@angular/compiler-cli": "^2.4.1",
    "@angular/platform-server": "^2.4.1",
    "@angular/core": "^2.3.0",
    "@angular/forms": "^2.3.0",
    "@angular/http": "^2.3.0",
    "@angular/platform-browser": "^2.3.0",
    "@angular/platform-browser-dynamic": "^2.3.0",
    "@angular/router": "^3.3.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-rc.5",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.35",
    "@types/node": "0.0.2",
    "angular2-in-memory-web-api": "0.0.21",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.9",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "es6-shim": "^0.35.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "lite-server": "^2.2.2",
    "raw-loader": "^0.5.1",
    "require": "^2.4.20",
    "rollup": "^0.38.0",
    "rollup-plugin-commonjs": "^6.0.1",
    "rollup-plugin-css-only": "^0.2.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "style-loader": "^0.13.1",
    "systemjs": "^0.19.41",
    "traceur": "0.0.111",
    "typescript": "^2.0.2",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
